{
  "name": "booking-system-backend",
  "version": "1.0.0",
  "description": "Calendar Booking System - Backend API",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "test": "NODE_ENV=test node --experimental-vm-modules ./node_modules/jest/bin/jest.js --coverage --runInBand",
    "test:unit": "NODE_ENV=test node --experimental-vm-modules ./node_modules/jest/bin/jest.js --testPathPattern=tests/unit",
    "test:integration": "NODE_ENV=test node --experimental-vm-modules ./node_modules/jest/bin/jest.js --testPathPattern=tests/integration",
    "test:e2e": "NODE_ENV=test node --experimental-vm-modules ./node_modules/jest/bin/jest.js --testPathPattern=tests/e2e",
    "test:watch": "NODE_ENV=test node --experimental-vm-modules ./node_modules/jest/bin/jest.js --watch",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "db:migrate": "node src/config/migrations/migrate.js",
    "db:seed": "node src/config/seeds/seed.js"
  },
  "keywords": [
    "booking",
    "calendar",
    "scheduling",
    "appointments"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "redis": "^4.6.12",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "cookie-parser": "^1.4.6",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "winston": "^3.11.0",
    "moment-timezone": "^0.5.44",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.1.1",
    "@faker-js/faker": "^8.3.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
